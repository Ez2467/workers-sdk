    "unenv": "npm:unenv-nightly@2.0.0-20241127-105629-7a9f230"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "eslint-config-unjs": "^0.4.1",
    "typescript": "catalog:default",
    "unbuild": "^2.0.0",
    "wrangler": "workspace:*"
  },
	"peerDependencies": {
		"workerd": "^1.20241112.0"
	},
	"peerDependenciesMeta": {
		"workerd": {
			"optional": true
		}
